{
    "type": "library",
    "name": "tienhm7/laravel-repository",
    "description": "Template for repository helper, library - Basic, Simple and Lightweight",
    "keywords": [
        "repository",
        "laravel",
        "helper",
        "library",
        "php"
    ],
    "homepage": "https://github.com/tienhm7/laravel-repository",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Hoang Manh Tien",
            "email": "tiencntt2@gmail.com",
            "homepage": "https://github.com/tienhm7",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "conflict": {
        "laravel/laravel": "<8.0"
    },
    "autoload": {
        "psr-4": {
            "tienhm7\\Repository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tienhm7\\Repository\\RepositoryProvider"
            ]
        }
    }
}
